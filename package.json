{
  "name": "@equinor/fusion-core",
  "version": "0.0.0",
  "description": "collection of fusion core packages",
  "private": true,
  "scripts": {
    "build": "pnpm -r build",
    "changeset:version": "changeset version && pnpm install --lockfile-only",
    "changeset:publish": "pnpm changeset tag && pnpm publish -r"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/equinor/fusion-core.git"
  },
  "keywords": [
    "fusion"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/equinor/fusion-core/issues"
  },
  "homepage": "https://github.com/equinor/fusion-core#readme",
  "engines": {
    "node": "^18 || ^20",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@9.1.0",
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1"
  }
}
