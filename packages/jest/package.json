{
  "name": "@equinor/jest-config-fusion",
  "version": "0.1.7",
  "keywords": ["fusion", "jest"],
  "author": "odin thomas rochmann <odin.rochmann@gmail.com>",
  "homepage": "https://github.com/equinor/fusion-core/tree/master/packages/jest#readme",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/equinor/fusion-core.git",
    "directory": "packages/jest"
  },
  "scripts": {
    "build": "tsc -b --clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --module 'ESNext' --outDir dist/esm",
    "build:cjs": "tsc --module 'CommonJS' --outDir dist/cjs",
    "prepack": "npm run build"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "index.d.ts",
  "typesVersions": {
    ">=4.2": {
      "*": [
        "dist/types/*"
      ]
    }
  },
  "peerDependencies": {
    "@types/jest": "26.x",
    "jest": "26.x",
    "ts-jest": "26.x",
    "ts-node": "9.x",
    "tslib": "2.x",
    "typescript": "4.x"
  },
  "peerDependenciesMeta": {
    "@types/jest": {
      "optional": true
    },
    "ts-node": {
      "optional": true
    }
  },
  "devDependencies": {
    "@jest/types": "^29.3.1",
    "@types/jest": "^26.0.15",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5"
  }
}
